    image 
      {
        function			= BOX_GAP
	recolorable		= TRUE
	file				= "Notebook/notebook.png" 
	border			= { 2, 2, 2, 2 }
	stretch			= TRUE
	gap_file			= "Notebook/gap_top.png"
